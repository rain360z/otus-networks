hostname S-NX9300_1

nv overlay evpn
feature ospf
feature bgp
feature vn-segment-vlan-based
feature bfd
feature nv overlay


username admin password 5 $5$JMEIOP$NluYln9P9sKC1EfF5WWC0FA61TnwCHggZTctjr79Ji7

bfd ipv4 interval 100 min_rx 100 multiplier 3

route-map LEAF permit 10
  match as-number 65001-65003
route-map NH_UNCHANGED permit 10
  set ip next-hop unchanged


interface Ethernet1/1
  no switchport
  no ip redirects
  ip address 172.16.1.0/31
  ip ospf authentication message-digest
  ip ospf message-digest-key 1 md5 3 01cee192a36f2db2
  no ip ospf passive-interface
  ip router ospf 10 area 0.0.0.0
  no shutdown

interface Ethernet1/2
  no switchport
  no ip redirects
  ip address 172.16.2.0/31
  ip ospf authentication message-digest
  ip ospf message-digest-key 1 md5 3 01cee192a36f2db2
  no ip ospf passive-interface
  ip router ospf 10 area 0.0.0.0
  no shutdown

interface Ethernet1/3
  no switchport
  ip address 172.16.3.0/31
  ip ospf authentication message-digest
  ip ospf message-digest-key 1 md5 3 01cee192a36f2db2
  no ip ospf passive-interface
  ip router ospf 10 area 0.0.0.0
  no shutdown


interface loopback0
  ip address 2.2.2.1/32
  ip router ospf 10 area 0.0.0.0

interface loopback100
  ip address 100.122.122.1/32
  ip router ospf 10 area 0.0.0.0

router ospf 10
  bfd
  router-id 2.2.2.1
  auto-cost reference-bandwidth 1000 Mbps
  passive-interface default
router bgp 64601
  router-id 2.2.2.1
  timers bgp 3 9
  reconnect-interval 12
  log-neighbor-changes
  address-family l2vpn evpn
    maximum-paths 10
    retain route-target all
  template peer LEAF
    update-source loopback0
    ebgp-multihop 5
    address-family l2vpn evpn
      send-community
      send-community extended
      route-map NH_UNCHANGED out
      rewrite-evpn-rt-asn
  neighbor 1.1.1.1
    inherit peer LEAF
    remote-as 65001
  neighbor 1.1.1.2
    inherit peer LEAF
    remote-as 65002
  neighbor 1.1.1.3
    inherit peer LEAF
    remote-as 65003
